<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="50dp" />
            <stroke android:width="1dip" android:color="#F8E546" />
            <gradient android:angle="-90" android:startColor="#F8E546" android:endColor="#E5D724"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="50dp" />
            <stroke android:width="1dip" android:color="#E5D724" />
            <solid android:color="#E5D724"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="50dp" />
            <stroke android:width="1dip" android:color="#F8E546" />
            <gradient android:angle="-90" android:startColor="#F8E546" android:endColor="#E5D724" />
        </shape>
    </item>
</selector>